{
  "name": "js-development-env",
  "version": "1.0.0",
  "description": "Javascript develeopment environment setup",
  "scripts": {
    "prestart": "babel-node build_scripts/start_message.js",
    "start": "npm-run-all --parallel security_check start_server watch:linting watch:test generate_mock_api:start",
    "start_server": "babel-node build_scripts/source_server.js",
    "security_check": "nsp check",
    "public_share": "lt --port 3000",
    "linting": "esw webpack.config.*, src, build_scripts --color",
    "watch:linting": "npm run linting -- --watch",
    "test": "mocha --reporter progress build_scripts/test_setup.js \"src/**/*.test.js\"",
    "watch:test": "npm run test -- --watch",
    "generate_mock_api:data": "babel-node build_scripts/mock_data_generator",
    "pregenerate_mock_api:start": "npm run generate_mock_api:data",
    "generate_mock_api:start": "json-server --watch src/api/db.json --port 3001"
  },
  "author": "Rakesh Verma",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.9.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.24.1",
    "jsdom": "9.8.3",
    "json-schema-faker": "0.3.7",
    "json-server": "0.8.23",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "9.0.2",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5",
     "whatwg-fetch": "^1.0.0"
  }
}
